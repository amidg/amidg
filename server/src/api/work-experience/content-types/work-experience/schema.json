{
  "kind": "collectionType",
  "collectionName": "work_experiences",
  "info": {
    "singularName": "work-experience",
    "pluralName": "work-experiences",
    "displayName": "Work Experience",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "companyTitle": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "string"
    },
    "companyLogo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid",
      "targetField": "companyTitle"
    },
    "order": {
      "type": "integer",
      "default": 1
    },
    "companyLink": {
      "type": "component",
      "repeatable": true,
      "component": "elements.link"
    }
